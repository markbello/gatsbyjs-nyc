  "version": "0.2.0",
  "contributors": [
    "Mark Bello <mark.bello.person@icloud.com>",
    "Oleg Chursin",
    "Denny Temple",
    "Edgar Ong"
  ],
  "dependencies": {
    "gatsby": "^2.32.13",
